{
  "name": "fake-news",
  "version": "1.0.0",
  "description": "This is a Node.js project which verfies the potential legitimacy of a source's information based on it's content and 'journalistic choices'.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "eslint . --ext .ts",
    "tsc": "tsc -p tsconfig.build.json",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iniffur/Fake-News.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iniffur/Fake-News/issues"
  },
  "homepage": "https://github.com/iniffur/Fake-News#readme",
  "dependencies": {
    "@types/express-flash": "^0.0.2",
    "body-parser": "^1.20.1",
    "bootstrap": "^5.2.2",
    "cookie-parser": "^1.4.6",
    "create-error": "^0.3.1",
    "createerror": "^1.3.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-router": "^0.0.1",
    "express-session": "^1.17.3",
    "mdb-ui-kit": "^6.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.8",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "fs-extra": "^10.1.0",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
